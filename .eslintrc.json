{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "prettier"
  ],
  "env": {
    "browser": true,
    "commonjs": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": "off",
    "@typescript-eslint/no-explicit-any": "warn",
    "valid-jsdoc": "warn"
  }
}
